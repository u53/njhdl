<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zb.njhld.dao.MerchantsMapper">
    
    <select id="findMerchants" resultType="merchants" parameterType="merchants">
        select  * from merchants
        where  merchantsName =#{merchantsName} and mergechantsPassword =#{merchantsPassword}
    </select>


</mapper>
